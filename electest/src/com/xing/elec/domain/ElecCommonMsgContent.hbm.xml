<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xing.elec.domain.ElecCommonMsgContent" table="Elec_CommonMsg_Content">
		<id name="comID" type="string" length="50" column="comID">
			<generator class="uuid"></generator>
		</id>
		<property name="type" type="string" length="5000" column="type"></property>
		<property name="content" type="string" length="5000" column="content"></property>
		<property name="orderby" type="integer" length="10" column="orderby"></property>
	</class>
</hibernate-mapping>
